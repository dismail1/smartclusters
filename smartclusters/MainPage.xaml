        <Button x:Name="dataManually" Content="Visualize data manually" HorizontalAlignment="Left" Margin="128,136,0,0" VerticalAlignment="Top" Width="156" Click="Button_Click_1" Height="46"/>
        <Button x:Name="dataManually_Copy" Content="Visualize data from API" HorizontalAlignment="Left" Margin="128,188,0,0" VerticalAlignment="Top" Width="156" Click="Button_Click_1" Height="45"/>
        
    </Grid>
